<Window x:Class="BLFClient.OptisetNew"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BLFClient"
        mc:Ignorable="d"
        Title="OptisetNew" Height="84.507" Width="215.845">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <CheckBox Name="UseOptisetExtension" Content="Use Optiset Extension" BorderThickness="2"/>
            <Label Content="Optiset Extension"/>
            <TextBox Name="OptisetExtension"/>
        </StackPanel>
        <StackPanel Grid.Column="1">
            <Button Content="Ok" Click="Button_Click_OK"/>
            <Button Content="Cancel" Click="Button_Click_Cancel"/>
        </StackPanel>
    </Grid>
</Window>
