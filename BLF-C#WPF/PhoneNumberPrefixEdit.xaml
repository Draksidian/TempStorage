<Window x:Class="BLFClient.PhoneNumberPrefixEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BLFClient"
        mc:Ignorable="d"
        Title="PhoneNumberPrefixEdit" Height="100" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Content="Prefix of Subscriber's number"/>
        <Button Content="Ok" Height="25" Grid.Row="0" Grid.Column="1" Click="Click_Ok"/>
        <TextBox Name="SubscriberPrefix" Height="25"  Grid.Row="1" Grid.Column="0"/>
        <Button Content="Cancel" Height="25" Grid.Row="1" Grid.Column="1" Click="Click_Cancel"/>
    </Grid>
</Window>
